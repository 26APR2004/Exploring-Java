class Arithmetic_Operator
{
    static public void main (String...arg)
    {
        
       int a= 10;
       int b=12;
       System.out.println(a+b);
       System.out.println(a-b);
       System.out.println(a*b);
       System.out.println(a/b);
       System.out.println(a%b);
       
    
    }
}
